    "html-to-pdfmake": "^2.4.4",
    "html2canvas": "^1.4.1",
    "i": "^0.3.7",
    "jquery": "^3.6.0",
    "jspdf": "^2.5.1",
    "jw-paginate": "^1.0.4",
    "jwt-decode": "^3.1.2",
    "katex": "^0.15.6",
    "nebular-icons": "^1.1.0",
    "ng-multiselect-dropdown": "^0.3.7",
    "ng2-charts": "^2.4.3",
    "ng2-completer": "^9.0.1",
